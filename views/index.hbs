
   

 <section>
    <div class="container  d-flex align-items-center mt-5 ml-4">
        <div class="row">
            {{#each products}}
            <div class="col-md-3">
                <div class="card  bg-secondary">
                    <img src="{{this.image}}" class="card-img-top" alt="Fissure in Sandstone" />
                    <div class="card-body">
                        <h5 class="card-title">{{this.name}}</h5>
                        <h5 class="card-title">{{this.Model}}</h5>
                        <p class="card-text">G-SHOCK Watches designed to resist mechanical shock and vibrations</p>
                        <a href="#!" class="btn btn-dark text-white">Add To Cart</a>
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</section>
 