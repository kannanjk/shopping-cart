<section>
    <div class="container bg-warning">
        <div class="row mt-4">
            <a href="/admin/add-products" class="btn btn-success ml-auto">Add products</a>
        </div>
        <table class="table mt-5">
  <thead>
    <tr>
      <th scope="col">no</th>
      <th scope="col">Model</th>
      <th scope="col">price</th>
       <th scope="col">price</th>
      <th scope="col">image</th>
    </tr>
  </thead>
  <tbody>
    {{#each products}}
    <tr>
      <th scope="row">1</th>
      <td>{{this.name}}</td>
      <td>{{this.Model}}</td>
      <td>{{this.prize}}</td>
      <td><img style="width: 50px;height: 50px;" src="/product-image/{{this._id}}.jpg" alt=""></td>
      <td>
        <a href="/admin/edit-products/{{this._id}}" class="btn btn-primary">edit</a>
        <a href="/admin/delete-product/{{this._id}}" onclick="return confirm('Are you want to delete {{this.name}}')" class="btn btn-danger">delete</a>
        </td>
    </tr>
    {{/each}}
  </tbody>
</table>
    </div>
</section>